{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-check-constants",
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-syntax-import-meta",
    "@babel/plugin-proposal-json-strings"
  ],
  "env": {
    "test": {
      "plugins": [
        "@babel/plugin-syntax-dynamic-import"
       ]
    }
  },
  "ignore": [
    "node_modules"
  ]
}
