{
  "name": "react-redux-boilerplate-2019",
  "version": "0.1.0",
  "description": "React boilerplate for react redux project",
  "main": "index.js",
  "author": "Jimmy Lee",
  "license": "UNLICENSED",
  "scripts": {
    "start": "jest && cross-env NODE_ENV=development webpack-dev-server -d --open --config ./webpack/webpack.config.js --hide-modules",
    "dev": "cross-env NODE_ENV=development webpack-dev-server -d --open --config ./webpack/webpack.config.js --display-error-detail",
    "watch": "cross-env NODE_ENV=development webpack -d --watch --config ./webpack/webpack.config.js --display-error-details",
    "build": "jest && cross-env NODE_ENV=development webpack -d --config ./webpack/webpack.config.js --display-error-details",
    "build:prod": "jest && webpack -p --config ./webpack/webpack.config.js --display-error-details",
    "build:silent": "jest --silent && webpack -p --config ./webpack/webpack.config.js --hide-modules",
    "test": "jest --verbose",
    "test:watch": "jest --watchAll",
    "test:update": "jest --update-snapshot --detectOpenHandles",
    "test:coverage": "jest --coverage",
    "test:silent": "jest --silent --clearCache",
    "update": "npm-check -u"
  },
  "keywords": [
    "react",
    "redux",
    "react-boilerplate"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-check-constants": "^7.0.0-beta.38",
    "@babel/plugin-proposal-json-strings": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-syntax-import-meta": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "autoprefixer": "^9.7.3",
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.1",
    "cssnano": "^4.1.10",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.7.2",
    "eslint-config-react-app": "^5.0.2",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^4.5.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "file-loader": "^5.0.2",
    "history": "^4.10.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "jest-plugin-context": "^2.9.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "npm-check": "^5.9.0",
    "postcss": "^7.0.23",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prop-types": "^15.7.2",
    "react-test-renderer": "^16.12.0",
    "recompose": "^0.30.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.1",
    "stylelint": "^12.0.0",
    "stylelint-scss": "^3.13.0",
    "stylelint-webpack-plugin": "^1.1.1",
    "svg-inline-loader": "^0.8.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  }
}
